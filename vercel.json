{
  "version": 2,
  "builds": [
    {
      "src": "backend/Dockerfile",
      "use": "@vercel/docker"
    },
    {
      "src": "frontend/Dockerfile",
      "use": "@vercel/docker",
      "config": {
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}